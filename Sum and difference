#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
	int int1,int2;
    float flt1, flt2;
    scanf("%d %d\n", &int1, &int2);
    scanf("%f %f",&flt1, &flt2);
    printf("%d %d\n", int1+int2, int1-int2);
    printf("%0.1f %0.1f", flt1+flt2, flt1-flt2);
    
    return 0;
}
